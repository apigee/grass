<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OAuthV2 async="false" continueOnError="false" enabled="true" name="generateAuthCode">
    <DisplayName>OAuth v2.0 1</DisplayName>
    <FaultRules/>
    <Operation>GenerateAuthorizationCode</Operation>
    <ExpiresIn>600000</ExpiresIn>
    <Scope>request.queryparam.scope</Scope>    
    <GenerateResponse/>
    <Attributes>
  		<Attribute name="userid" ref="request.queryparam.userid" display="true" />
  		<Attribute name="sessionid" ref="request.queryparam.sessionid" display="true" />
  		<Attribute name="nonce" ref="request.queryparam.nonce" display="true" />
  		<Attribute name="scope" ref="request.queryparam.scope" display="true" />
 	</Attributes>
</OAuthV2>

