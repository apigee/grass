<?xml version="1.0" encoding="UTF-8"?><AssignMessage name="redirectToConsent">
	<AssignTo createNew="true" type="response">response</AssignTo>
	<IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
	
	<Remove>
		<Payload/>
	</Remove>
	<Set>
		 <StatusCode>302</StatusCode>
		<Headers> 
			<Header name="Location">[TO_BE_REPLACED]</Header> 
			<Header name="Access-Control-Allow-Origin">*</Header>
			<Header name="Access-Control-Allow-Origin">*</Header>
			<Header name="Set-cookie">sessionid= {sessionid}; Max-Age=18000; Version=1; path=/</Header>
			
		</Headers> 
	</Set>
</AssignMessage>