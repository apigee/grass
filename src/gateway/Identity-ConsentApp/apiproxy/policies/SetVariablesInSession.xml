	<Set>

		<Payload contentType="application/json" variablePrefix="%"
			variableSuffix="#">
			{"client_id" : "%client_id#",
			"response_type" : "%response_type#",
			"scope" : "%scope#",
			"userid" : "%userid#" ,
			 "password":"%password#", 
			 "redirect_uri":"%redirect_uri#"
			 ,"pre_page_id":"%pre_page_id#"
			 ,"create_msisdn":"%create_msisdn#"
			 ,"create_email":"%create_email#"
			 ,"create_name":"%create_name#"
			 ,"create_lastName":"%create_lastName#"
			 ,"create_username":"%create_username#"
			 ,"create_password":"%create_password#"
			 ,"registrationFlag":"%registrationFlag#"
			 ,"socialLoginDetails":"%socialLoginDetails#"
			 ,"pinValidation":"%pinValidation#"
			 ,"previousflow":"%previousflow#"
			 ,"pinEnteryNo":"%pinEnteryNo#"
			 ,"resetFalg":"%resetFalg#"
			 ,"reset_username":"%reset_username#"
			 ,"reset_password":"%reset_password#"
			 ,"nonce":"%nonce#"
			 ,"display":"%display#"
			 ,"req_state":"%req_state#"
			 ,"appName":"%appName#"
			 ,"isUserProfileFlow":"%isUserProfileFlow#"
			 ,"open_id":"%open_id#"
			 ,"msisdn": "%msisdn#"
			 ,"areMaxTokenTriesExceeded": "%areMaxTokenTriesExceeded#"
