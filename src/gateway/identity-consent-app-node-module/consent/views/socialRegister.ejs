
      <form name="input" class="form-signin" action="/openid/socialRegistration?sessionid=<%= sessionid%>&querystring=<%= querystring%>" method="post" style="padding-top: 60px;">
        <h2 class="form-signin-heading">Register</h2>

			First Name: <input type="text" name="name" value="<%= name%>"><br>
			Last Name: <input type="text" name="lastName" value="<%= surname%>"><br>
			Email :&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="text" name="emailId" value="<%= email%>"><br>
      		Phone No: &nbsp; <input type="text" name="msisdn" value="<%= msisdn%>"><br>
       

 		  <fieldset class="ui-grid-a">
				<div class="ui-block-a"> <button class="btn btn-medium btn-primary" type="submit" onclick="return validateForm();return false;">Submit</button></div>
				<div class="ui-block-b"><button class="btn btn-medium btn-error" type="button" onclick="window.location='<%= redirectURL%>'" >Cancel</button></div>
		    </fieldset>
        
      </form>
    
    
    
    <script>
      function validateForm() {
			var email=document.forms["input"]["emailId"].value;
			var msisdn=document.forms["input"]["msisdn"].value;
			
	
		
			 if (!Boolean(msisdn)){
			alert("Mobile number can not be blank");
			return false;
			}
			else if(!Boolean(email)){
					alert("Email can not be blank");
			return false;
			}
			
			}
    
        </script>
    
    
    
