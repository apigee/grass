<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables enabled="true" continueOnError="false" async="false" name="ExtractUserProfileResponse">
	<DisplayName>ExtractUserProfileResponse</DisplayName>
    <FaultRules/>
    <Properties/>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <Source>appservicesuserprofile</Source>

    <JSONPayload> 
		 <Variable name="name">  
			<JSONPath>$.personal-info.name.given</JSONPath>
		</Variable>
		 <Variable name="family_name">
			<JSONPath>$.personal-info.name.surname</JSONPath>
		</Variable>
		<Variable name="given_name">
			<JSONPath>$.personal-info.name.given</JSONPath>
		</Variable>
		<Variable name="username">
			<JSONPath>$.username</JSONPath>
		</Variable>
		<Variable name="picture">
			<JSONPath>$.personal-info.image</JSONPath>
		</Variable>
		<Variable name="website">
			<JSONPath>$.website</JSONPath>
		</Variable>
		<Variable name="gender">
			<JSONPath>$.personal-info.gender</JSONPath>
		</Variable>
		<Variable name="birthdate">
			<JSONPath>$.personal-info.dob</JSONPath>
		</Variable>
		<Variable name="zoneinfo">
			<JSONPath>$.zoneinfo</JSONPath>
		</Variable>
		<Variable name="locale">
			<JSONPath>$.locale</JSONPath>
		</Variable>
		<Variable name="updated_at">
			<JSONPath>$.modified</JSONPath>
		</Variable>
		<Variable name="email">
			<JSONPath>$.email</JSONPath>
		</Variable>
		<Variable name="email_verified">
			<JSONPath>$.email</JSONPath>
		</Variable>
		<Variable name="phone_number">
			<JSONPath>$.msisdn</JSONPath>
		</Variable>
    </JSONPayload>
</ExtractVariables>

